
<%= render "shared/header" %>
<div class="contents">
  <div class="section">
    <div class="schedule_head">
      <p class="left-click" id="left-click"> 
       <%= link_to image_tag("left-direction.png", class:"click-icon"), "/" %>
      </p>
      <p class="schedule-date" id="schedule-date">
       <%= link_to "日にち （曜日）", "#", class:"schedule-today" %>
